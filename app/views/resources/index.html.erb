
<h1> Browse Resources </h1>
<%= @current_uri %>
<div class="pure-g">
    
	 
	<% @resources.each do |resource| %>
		<div class="pure-u-1-3">
			<div class="thumb">
				<h2><%= resource.title %></h2>
				<p><%= resource.description %></p>
				<p><%= resource.link %></p>
			</div>
		</div>
	<% end %>
</div>
